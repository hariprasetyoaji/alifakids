						ON s.student_id = p.student_id AND month(p.period) = month('2021-01-07') AND year(p.period) = year('2021-01-07')
				LEFT OUTER JOIN ak_branch b 
						ON s.branch_id = b.branch_id
				LEFT OUTER JOIN ak_class c 
						ON s.class_id = c.class_id
				LEFT JOIN ak_parents_students ps
						ON ps.student_id = s.student_id
				LEFT JOIN ak_users u
						ON u.ID = ps.parent_id
			
		 AND ak_students.name LIKE '%Fazel%' AND s.class_id LIKE '%%' AND s.branch_id LIKE '%%'  GROUP BY s.student_id  ORDER BY s.name ASC  LIMIT 20 OFFSET 0 yang dibuat oleh do_action('toplevel_page_payment'), WP_Hook->do_action, WP_Hook->apply_filters, payment_page_handler, Payment_List->prepare_items
